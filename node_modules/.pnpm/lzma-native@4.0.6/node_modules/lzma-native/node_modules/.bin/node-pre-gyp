#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules/node-pre-gyp/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node-pre-gyp@0.11.0/node_modules:/mnt/e/Documentos/Github/AplicativoFIber/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
else
  exec node  "$basedir/../../../../../node-pre-gyp@0.11.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
fi
